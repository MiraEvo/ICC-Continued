<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!-- 设置面板资源字典 -->
    <!-- 包含设置面板相关的样式、图标和动画 -->
    
    <!-- ==================== 颜色定义 ==================== -->
    <SolidColorBrush x:Key="SettingsPanelBackground" Color="#ee18181b"/>
    <SolidColorBrush x:Key="SettingsHeaderBackground" Color="#18181b"/>
    <SolidColorBrush x:Key="SettingsNavBackground" Color="#AA09090b"/>
    <SolidColorBrush x:Key="SettingsTextForeground" Color="#fafafa"/>
    <SolidColorBrush x:Key="SettingsSecondaryText" Color="#a1a1aa"/>
    <SolidColorBrush x:Key="SettingsAccentColor" Color="#3b82f6"/>
    <SolidColorBrush x:Key="SettingsBorderColor" Color="#a1a1aa"/>
    <SolidColorBrush x:Key="SettingsDividerColor" Color="#3f3f46"/>
    
    <!-- ==================== 导航按钮样式 ==================== -->
    
    <!-- 设置导航按钮基础样式 -->
    <Style x:Key="SettingsNavButtonStyle" TargetType="Border">
        <Setter Property="Width" Value="50"/>
        <Setter Property="Height" Value="50"/>
        <Setter Property="BorderBrush" Value="#3b82f6"/>
        <Setter Property="BorderThickness" Value="0,0,0,0"/>
        <Setter Property="Cursor" Value="Hand"/>
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background" Value="#20ffffff"/>
            </Trigger>
        </Style.Triggers>
    </Style>
    
    <!-- 选中的导航按钮样式 -->
    <Style x:Key="SettingsNavButtonSelectedStyle" TargetType="Border" BasedOn="{StaticResource SettingsNavButtonStyle}">
        <Setter Property="BorderThickness" Value="0,0,4,0"/>
    </Style>
    
    <!-- 导航图标样式 -->
    <Style x:Key="SettingsNavIconStyle" TargetType="Image">
        <Setter Property="Height" Value="28"/>
        <Setter Property="Width" Value="28"/>
        <Setter Property="RenderOptions.BitmapScalingMode" Value="HighQuality"/>
    </Style>
    
    <!-- ==================== 设置面板容器样式 ==================== -->
    
    <!-- 设置面板主容器样式 -->
    <Style x:Key="SettingsPanelContainerStyle" TargetType="Border">
        <Setter Property="Background" Value="#ee18181b"/>
        <Setter Property="Width" Value="490"/>
    </Style>
    
    <!-- 设置面板头部样式 -->
    <Style x:Key="SettingsHeaderStyle" TargetType="Border">
        <Setter Property="Background" Value="#18181b"/>
        <Setter Property="CornerRadius" Value="0"/>
        <Setter Property="Margin" Value="-1,-1,-1,0"/>
    </Style>
    
    <!-- 设置面板标题文本样式 -->
    <Style x:Key="SettingsTitleTextStyle" TargetType="TextBlock">
        <Setter Property="Foreground" Value="White"/>
        <Setter Property="FontWeight" Value="Bold"/>
        <Setter Property="FontSize" Value="32"/>
        <Setter Property="Margin" Value="25,-2,0,0"/>
    </Style>
    
    <!-- 操作按钮区域边框样式 -->
    <Style x:Key="SettingsActionBorderStyle" TargetType="Border">
        <Setter Property="Margin" Value="0,0,0,10"/>
        <Setter Property="Height" Value="100"/>
        <Setter Property="CornerRadius" Value="5"/>
        <Setter Property="BorderBrush" Value="#a1a1aa"/>
        <Setter Property="BorderThickness" Value="1"/>
    </Style>
    
    <!-- ==================== 按钮样式 ==================== -->
    
    <!-- 设置操作按钮样式 -->
    <Style x:Key="SettingsActionButtonStyle" TargetType="Button">
        <Setter Property="Width" Value="116"/>
        <Setter Property="Height" Value="45"/>
        <Setter Property="FontFamily" Value="Microsoft YaHei UI"/>
        <Setter Property="Cursor" Value="Hand"/>
    </Style>
    
    <!-- 按钮图标样式 -->
    <Style x:Key="SettingsButtonIconStyle" TargetType="Image">
        <Setter Property="RenderOptions.BitmapScalingMode" Value="HighQuality"/>
        <Setter Property="Height" Value="20"/>
        <Setter Property="Width" Value="20"/>
        <Setter Property="Margin" Value="0,0,6,0"/>
    </Style>
    
    <!-- 按钮文本样式 -->
    <Style x:Key="SettingsButtonTextStyle" TargetType="Label">
        <Setter Property="FontSize" Value="16"/>
        <Setter Property="Foreground" Value="#fafafa"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="FontFamily" Value="Microsoft YaHei UI"/>
        <Setter Property="FontWeight" Value="Bold"/>
    </Style>
    
    <!-- ==================== 分组框样式 ==================== -->
    
    <!-- 设置分组标题英文文本样式 -->
    <Style x:Key="SettingsGroupEnglishTitleStyle" TargetType="TextBlock">
        <Setter Property="Foreground" Value="#fafafa"/>
        <Setter Property="Opacity" Value="0.1"/>
        <Setter Property="FontSize" Value="20"/>
    </Style>
    
    <!-- 设置分组标题中文文本样式 -->
    <Style x:Key="SettingsGroupChineseTitleStyle" TargetType="TextBlock">
        <Setter Property="FontWeight" Value="Bold"/>
        <Setter Property="Foreground" Value="#fafafa"/>
        <Setter Property="FontSize" Value="26"/>
    </Style>
    
    <!-- 设置项标签样式 -->
    <Style x:Key="SettingsLabelStyle" TargetType="TextBlock">
        <Setter Property="Foreground" Value="#fafafa"/>
        <Setter Property="FontSize" Value="14"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
    </Style>
    
    <!-- 设置项描述文本样式 -->
    <Style x:Key="SettingsDescriptionStyle" TargetType="TextBlock">
        <Setter Property="Foreground" Value="#a1a1aa"/>
        <Setter Property="FontSize" Value="12"/>
        <Setter Property="TextWrapping" Value="Wrap"/>
    </Style>
    
