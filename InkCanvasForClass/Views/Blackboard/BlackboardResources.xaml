    <!-- 白板按钮基础样式 -->
    <Style x:Key="BlackboardButtonStyle" TargetType="Border">
        <Setter Property="Background" Value="#f4f4f5"/>
        <Setter Property="BorderBrush" Value="#a1a1aa"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Opacity" Value="0.95"/>
        <Setter Property="Cursor" Value="Hand"/>
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background" Value="#e4e4e7"/>
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Opacity" Value="0.5"/>
            </Trigger>
        </Style.Triggers>
    </Style>
    
    <!-- 白板导航按钮样式 -->
    <Style x:Key="BlackboardNavButtonStyle" TargetType="Border" BasedOn="{StaticResource BlackboardButtonStyle}">
        <Setter Property="Width" Value="60"/>
        <Setter Property="Height" Value="50"/>
    </Style>
    
    <!-- 白板页面计数按钮样式 -->
    <Style x:Key="BlackboardPageCountButtonStyle" TargetType="Border" BasedOn="{StaticResource BlackboardButtonStyle}">
        <Setter Property="Width" Value="75"/>
        <Setter Property="Height" Value="50"/>
    </Style>
    
    <!-- 白板按钮文本样式 -->
    <Style x:Key="BlackboardButtonTextStyle" TargetType="TextBlock">
        <Setter Property="Foreground" Value="#18181b"/>
        <Setter Property="FontSize" Value="12"/>
        <Setter Property="VerticalAlignment" Value="Bottom"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
    </Style>
    
    <!-- 白板页面计数文本样式 -->
    <Style x:Key="BlackboardPageCountTextStyle" TargetType="TextBlock">
        <Setter Property="FontSize" Value="17"/>
        <Setter Property="FontWeight" Value="Bold"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="TextAlignment" Value="Center"/>
    </Style>
    
    <!-- 页面列表弹出层样式 -->
    <Style x:Key="PageListPopupStyle" TargetType="Border">
        <Setter Property="CornerRadius" Value="8"/>
        <Setter Property="Background" Value="#fafafa"/>
        <Setter Property="BorderBrush" Value="#a1a1aa"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="ClipToBounds" Value="True"/>
    </Style>
    
    <!-- 页面缩略图边框样式 -->
    <Style x:Key="PageThumbnailBorderStyle" TargetType="Border">
        <Setter Property="Margin" Value="0,4"/>
        <Setter Property="Width" Value="160"/>
        <Setter Property="BorderBrush" Value="#a1a1aa"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Cursor" Value="Hand"/>
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="BorderBrush" Value="#2563eb"/>
                <Setter Property="BorderThickness" Value="2"/>
            </Trigger>
        </Style.Triggers>
    </Style>
    
    <!-- 页面索引标签样式 -->
    <Style x:Key="PageIndexLabelStyle" TargetType="Border">
        <Setter Property="Margin" Value="4"/>
        <Setter Property="Width" Value="36"/>
        <Setter Property="Height" Value="20"/>
        <Setter Property="CornerRadius" Value="3"/>
        <Setter Property="HorizontalAlignment" Value="Left"/>
        <Setter Property="VerticalAlignment" Value="Top"/>
        <Setter Property="Background" Value="#cc09090b"/>
    </Style>
    
    <!-- 页面索引文本样式 -->
    <Style x:Key="PageIndexTextStyle" TargetType="TextBlock">
        <Setter Property="FontSize" Value="14"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="FontFamily" Value="Consolas"/>
        <Setter Property="Foreground" Value="White"/>
    </Style>
    
    <!-- 背景颜色选项样式 -->
    <Style x:Key="BackgroundColorOptionStyle" TargetType="Border">
        <Setter Property="Width" Value="46"/>
        <Setter Property="Height" Value="46"/>
        <Setter Property="CornerRadius" Value="4"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Cursor" Value="Hand"/>
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="BorderThickness" Value="2"/>
            </Trigger>
        </Style.Triggers>
    </Style>
    
    <!-- 弹出面板标题样式 -->
    <Style x:Key="PopupTitleBorderStyle" TargetType="Border">
        <Setter Property="BorderBrush" Value="#1e3a8a"/>
        <Setter Property="BorderThickness" Value="0,0,0,1"/>
        <Setter Property="CornerRadius" Value="8,8,0,0"/>
        <Setter Property="Background" Value="#2563eb"/>
        <Setter Property="Margin" Value="-1,-1,-1,1"/>
    </Style>
    
    <Style x:Key="PopupTitleTextStyle" TargetType="TextBlock">
        <Setter Property="Foreground" Value="White"/>
        <Setter Property="Padding" Value="0,7"/>
        <Setter Property="FontSize" Value="17"/>
        <Setter Property="FontWeight" Value="Bold"/>
        <Setter Property="TextAlignment" Value="Center"/>
    </Style>
    
</ResourceDictionary>
